# Supabase Configuration (Client-side accessible)
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Server-side only environment variables (for backend services)
S3_STORAGE_URL=your_s3_storage_url
S3_REGION=your_s3_region
S3_ACCESS_ID=your_s3_access_id
S3_ACCESS_KEY=your_s3_access_key

# OpenAI Configuration (Client-side accessible)
EXPO_PUBLIC_OPENAI_API_KEY=your_openai_api_key

# Optional API Configuration
EXPO_PUBLIC_API_BASE_URL=https://your-api-url.com

# CDN Configuration (Optional)
CDN_BASE_URL=https://your-cdn-domain.com
CDN_ENABLED=true

# Environment
NODE_ENV=development